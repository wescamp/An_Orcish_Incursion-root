msgid "An Orcish Incursion"
msgstr ""

#: An_Orcish_Incursion.cfg:13
msgid "(hardest)"
msgstr ""

#: An_Orcish_Incursion.cfg:13
msgid "High Lord"
msgstr ""

#: An_Orcish_Incursion.cfg:13
